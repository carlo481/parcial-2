package uspg.edu;

/**
 *
 * @author carlos
 */
public class parcial {
    
    int id;
    String nombre;
    double serie;
    double direccion;
    double fecha;
    double Nit;
    double total;
    
    Parcial(){}
    
    Parcial(int id, String nombre, double serie, 
            double Nombre, double fecha, 
            double Nit){
    
        this.id = id;
        this.nombre = nombre;
        this.serie = serie;
        this.direccion = direccion;
        this.fecha = fecha;
        this.Nit = Nit;
        this.total = serie+Nombre+direccion+fecha;  
    }

    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

    public String getNombre() {
        return nombre;
    }

    public void setNombre(String nombre) {
        this.nombre = nombre;
    }

    public double getParcialUno() {
        return serie;
    }

    public void setserie(double serie) {
        this.Nombre = Nombre;
        actualizarTotal();
    }

    public double Nombre() {
        return Nombre;
    }

    public void setserie(double serie) {
        this.serie = serie;
        actualizarTotal();
    }

    public double getdireccion() {
        return direccion;
    }

    public void setdireccion(double direcccion) {
        this.direccion = direccion;
        actualizarTotal();
    }

    public double getfecha() {
        return fecha;
    }

    public void setfecha(double fecha) {
        this.fecha = fecha;
        actualizarTotal();
    }

    public double getTotal() {
        return total;
    }

    public void setTotal(double total) {
        this.total = total;
    }
    
    public void actualizarTotal(){
        setTotal(serie+nombre+direccion+fecha);  
    }
    
}


